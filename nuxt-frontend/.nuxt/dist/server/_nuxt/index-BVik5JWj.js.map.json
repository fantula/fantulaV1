AAA,CAAA;AAAA,sBAAH,CAAA;AAAA;;;;;;kBAIhB,OAAA;AAAA,kBAAtD,OAAA,EAAA,cAAA,OAAA;AAAA,gBAAgB,GAAA,QAAA;AAAA,gBAAgCR,YAAA,4BAAA;AAAA,kBASzC,QAAA;AAAA,kBARS,QAAA;AAAA,kBAAG,MAAA;AAAA,gBAAY,GAAA;AAAA;oBACyCA,YAAA,iCAAA,EAAA,OAAA,OAAA,GAAA;AAAA,sBAA/C,SAAAD,QAAA,MAAA;AAAA,wBAAwBK,gBAAAI,gBAAA,WAAA,MAAA,MAAA,GAAA,CAAA;AAAA,sBAAH,CAAA;AAAA;;;sBACrB,SAAAT,QAAA,MAAA;AAAA,wBACgEC,YAAA,mBAAA;AAAA,0BAAA,MAAA,aAAA,WAAA,MAAA,MAAA;AAAA,wBAAlD,GAAA;AAAA;4BAAyCI,gBAAAI,gBAAA,cAAA,WAAA,MAAA,MAAA,CAAA,GAAA,CAAA;AAAA,0BAAJ,CAAA;AAAA;;;;;;sBAI9D,KAAA;AAAA;oBAFK,GAAA;AAAA;wBACyCR,YAAA,QAAA,EAAA,OAAA,EAAA,SAAA,YAAA,GAAAQ,gBAAA,WAAA,MAAA,YAAA,GAAA,CAAA;AAAA,sBAAV,CAAA;AAAA;;;;;;;;;;;;;QAkBtD,uBAAA,CAAA,WAAA,oBAAA,QAAA;AAAA,QAAmB,OAAA;AAAA,QACtB,OAAA;AAAA,QACA,kBAAA;AAAA,MACN,GAAA;AAAA;;;;;YAc0C,GAAA;AAAA;;;;;;kBAAY;AAAA;;;;;;cAClC,SAAA;AAAA,YAAiB,GAAA;AAAA;;;;;;kBAAmB;AAAA;;;;;;;;gBAF3BR,YAAA,sBAAA;AAAA,kBACqC,SAAA,CAAA,WAAA,oBAAA,QAAA;AAAA,gBAAxB,GAAA;AAAA;oBAAYI,gBAAA,IAAA;AAAA,kBAAA,CAAA;AAAA;;;kBACc,MAAA;AAAA,kBAAhD,SAAA;AAAA,gBAAiB,GAAA;AAAA;oBAAmBA,gBAAA,MAAA;AAAA,kBAAA,CAAA;AAAA;;;;;;;;;;cAbvC,eAAA;AAAA,YAAwB,GAAA;AAAA;;AACzB,yBAAAK,mBAAA,yBAAA,EAAA,OAAA,OAAA,GAAA;AAAA,oBAAY,SAAAV,QAAA,CAAA,IAAA,QAAA,UAAA,cAAA;;;;0BAEF,uBAAA,CAAA,WAAA,WAAA,SAAA;AAAA,0BAAM,MAAA;AAAA,0BACrB,MAAA;AAAA,0BACE,aAAA;AAAA,wBACK,GAAA,MAAA,UAAA,SAAA,CAAA;AAAA;;;4BACZ,YAAA,WAAA;AAAA,4BAJoB,uBAAA,CAAA,WAAA,WAAA,SAAA;AAAA,4BAAM,MAAA;AAAA,4BACrB,MAAA;AAAA,4BACE,aAAA;AAAA,0BACK,GAAA,MAAA,GAAA,CAAA,cAAA,qBAAA,CAAA;AAAA;;;;;;;;sBALU,SAAAA,QAAA,MAAA;AAAA,wBAMtBC,YAAA,qBAAA;AAAA,0BAAA,YAAA,WAAA;AAAA,0BAJoB,uBAAA,CAAA,WAAA,WAAA,SAAA;AAAA,0BAAM,MAAA;AAAA,0BACrB,MAAA;AAAA,0BACE,aAAA;AAAA,wBACK,GAAA,MAAA,GAAA,CAAA,cAAA,qBAAA,CAAA;AAAA;;;;;;;;;;;gBAGR,OAAA;AAAA,gBATO,eAAA;AAAA,cAAwB,GAAA;AAAA;kBAQxBA,YAAA,yBAAA,EAAA,OAAA,OAAA,GAAA;AAAA,oBAPW,SAAAD,QAAA,MAAA;AAAA,sBAMtBC,YAAA,qBAAA;AAAA,wBAAA,YAAA,WAAA;AAAA,wBAJoB,uBAAA,CAAA,WAAA,WAAA,SAAA;AAAA,wBAAM,MAAA;AAAA,wBACrB,MAAA;AAAA,wBACE,aAAA;AAAA,sBACK,GAAA,MAAA,GAAA,CAAA,cAAA,qBAAA,CAAA;AAAA;;;;;;;;;;;;;QAcT,uBAAA,CAAA,WAAA,mBAAA,QAAA;AAAA,QAAkB,OAAA;AAAA,QACrB,OAAA;AAAA,QACA,kBAAA;AAAA,MACN,GAAA;AAAA;;;;;YAKyC,GAAA;AAAA;;;;;;kBAAY;AAAA;;;;;;cACjC,SAAA;AAAA,YAAkB,GAAA;AAAA;;;;;;kBAAkB;AAAA;;;;;;;;gBAF3BA,YAAA,sBAAA;AAAA,kBACoC,SAAA,CAAA,WAAA,mBAAA,QAAA;AAAA,gBAAxB,GAAA;AAAA;oBAAYI,gBAAA,IAAA;AAAA,kBAAA,CAAA;AAAA;;;kBACe,MAAA;AAAA,kBAAhD,SAAA;AAAA,gBAAkB,GAAA;AAAA;oBAAkBA,gBAAA,MAAA;AAAA,kBAAA,CAAA;AAAA;;;;;;;;;;;;YAJ1B;AAAA;;;;;;;;;;;;;;;","names":["_ssrRenderAttrs","_mergeProps","_withCtx","_createVNode","_unref","Search","_withKeys","_createTextVNode","Refresh","_ssrGetDirectiveProps","_ssrInterpolate","_toDisplayString","_ssrRenderComponent"],"sources":["../../../../pages/_mgmt_9Xfa3/refunds/index.vue"],"sourcesContent":["<template>\n  <div class=\"refund-page\">\n    <div class=\"page-header\">\n      <h2>退款管理</h2>\n    </div>\n\n    <!-- 筛选 -->\n    <el-card shadow=\"never\" class=\"filter-card\">\n      <div class=\"filter-container\">\n        <div class=\"filter-left\">\n          <el-select v-model=\"filterParams.status\" placeholder=\"退款状态\" clearable style=\"width: 150px; margin-right: 15px;\" @change=\"handleSearch\">\n            <el-option label=\"待处理\" value=\"pending\" />\n            <el-option label=\"已驳回\" value=\"rejected\" />\n            <el-option label=\"已退款\" value=\"refunded\" />\n          </el-select>\n          <el-input v-model=\"filterParams.uid\" placeholder=\"用户UID\" clearable style=\"width: 200px; margin-right: 15px;\" @keyup.enter=\"handleSearch\">\n             <template #prefix>\n              <el-icon><Search /></el-icon>\n            </template>\n          </el-input>\n          <el-button type=\"primary\" :icon=\"Search\" @click=\"handleSearch\">查询</el-button>\n          <el-button :icon=\"Refresh\" @click=\"handleRefresh\">刷新</el-button>\n        </div>\n      </div>\n    </el-card>\n\n    <!-- 列表 -->\n    <el-card shadow=\"never\" class=\"list-card\">\n      <el-table :data=\"refundList\" style=\"width: 100%\" v-loading=\"loading\" border>\n        <el-table-column prop=\"refundNo\" label=\"退款编号\" min-width=\"180\" show-overflow-tooltip />\n        <el-table-column prop=\"orderNo\" label=\"关联订单号\" min-width=\"180\" show-overflow-tooltip />\n        <el-table-column prop=\"uid\" label=\"用户UID\" width=\"120\" />\n        <el-table-column label=\"退款金额\" width=\"120\">\n          <template #default=\"{ row }\">\n            ¥{{ row.amount }}\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"applyTime\" label=\"申请时间\" width=\"180\" />\n        <el-table-column label=\"状态\" width=\"120\" align=\"center\">\n          <template #default=\"{ row }\">\n            <el-tag :type=\"getStatusTag(row.status)\">{{ getStatusName(row.status) }}</el-tag>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"操作\" width=\"120\" fixed=\"right\" align=\"center\">\n          <template #default=\"{ row }\">\n            <el-button link type=\"primary\" @click=\"handleDetail(row)\">\n              {{ row.status === 'pending' ? '处理' : '查看' }}\n            </el-button>\n          </template>\n        </el-table-column>\n      </el-table>\n\n       <!-- 分页 -->\n      <div class=\"pagination-wrapper\" style=\"margin-top: 20px; display: flex; justify-content: flex-end;\">\n        <el-pagination\n          background\n          layout=\"prev, pager, next\"\n          :total=\"100\"\n        />\n      </div>\n    </el-card>\n\n    <!-- 处理/查看弹窗 -->\n    <el-dialog\n      v-model=\"dialogVisible\"\n      :title=\"currentRow?.status === 'pending' ? '处理退款申请' : '查看退款详情'\"\n      width=\"700px\"\n      @close=\"resetDialog\"\n    >\n      <div v-if=\"currentRow\" class=\"detail-container\">\n        <!-- 订单信息 -->\n        <div class=\"section-title\">订单信息</div>\n        <el-descriptions :column=\"2\" border size=\"small\">\n          <el-descriptions-item label=\"订单号\">{{ currentRow.orderInfo.orderNo }}</el-descriptions-item>\n          <el-descriptions-item label=\"实付金额\">¥{{ currentRow.orderInfo.paidAmount }}</el-descriptions-item>\n          <el-descriptions-item label=\"商品名称\" :span=\"2\">{{ currentRow.orderInfo.productName }}</el-descriptions-item>\n          <el-descriptions-item label=\"商品规格\" :span=\"2\">{{ currentRow.orderInfo.productSpec }}</el-descriptions-item>\n          <el-descriptions-item label=\"支付时间\">{{ currentRow.orderInfo.payTime }}</el-descriptions-item>\n          <el-descriptions-item label=\"支付方式\">{{ currentRow.orderInfo.paymentMethod }}</el-descriptions-item>\n          <el-descriptions-item label=\"过期时间\" :span=\"2\">{{ currentRow.orderInfo.expiryTime }}</el-descriptions-item>\n        </el-descriptions>\n\n        <!-- 退款信息 -->\n        <div class=\"section-title\" style=\"margin-top: 20px;\">退款申请信息</div>\n         <el-descriptions :column=\"1\" border size=\"small\">\n          <el-descriptions-item label=\"退款理由\">{{ currentRow.reason }}</el-descriptions-item>\n          <el-descriptions-item label=\"当前状态\">\n             <el-tag :type=\"getStatusTag(currentRow.status)\">{{ getStatusName(currentRow.status) }}</el-tag>\n          </el-descriptions-item>\n          <el-descriptions-item label=\"驳回原因\" v-if=\"currentRow.status === 'rejected' && currentRow.rejectReason\">\n             <span style=\"color: #F56C6C;\">{{ currentRow.rejectReason }}</span>\n          </el-descriptions-item>\n        </el-descriptions>\n      </div>\n\n      <template #footer>\n        <span class=\"dialog-footer\">\n          <el-button @click=\"dialogVisible = false\">关闭</el-button>\n          <template v-if=\"currentRow?.status === 'pending'\">\n             <el-button type=\"danger\" @click=\"handleReject\">驳回退款</el-button>\n             <el-button type=\"primary\" @click=\"handleAgree\">同意退款</el-button>\n          </template>\n        </span>\n      </template>\n    </el-dialog>\n\n    <!-- 驳回退款弹窗 (Nested) -->\n    <el-dialog\n      v-model=\"rejectDialogVisible\"\n      title=\"驳回退款\"\n      width=\"400px\"\n      append-to-body\n    >\n      <el-form :model=\"rejectForm\" label-width=\"80px\">\n        <el-form-item label=\"驳回原因\">\n          <el-input \n            v-model=\"rejectForm.reason\" \n            type=\"textarea\" \n            :rows=\"3\" \n            placeholder=\"请输入驳回原因\" \n          />\n        </el-form-item>\n      </el-form>\n      <template #footer>\n        <span class=\"dialog-footer\">\n          <el-button @click=\"rejectDialogVisible = false\">取消</el-button>\n          <el-button type=\"danger\" @click=\"confirmReject\">确认驳回</el-button>\n        </span>\n      </template>\n    </el-dialog>\n\n    <!-- 同意退款确认弹窗 (Nested) -->\n    <el-dialog\n      v-model=\"agreeDialogVisible\"\n      title=\"确认同意退款\"\n      width=\"400px\"\n      append-to-body\n    >\n      <span>确认同意该退款申请吗？操作后资金将退回用户钱包。</span>\n      <template #footer>\n        <span class=\"dialog-footer\">\n          <el-button @click=\"agreeDialogVisible = false\">取消</el-button>\n          <el-button type=\"primary\" @click=\"confirmAgree\">确认同意</el-button>\n        </span>\n      </template>\n    </el-dialog>\n  </div>\n</template>\n\n<script setup lang=\"ts\">\nimport { ref, reactive } from 'vue'\nimport { Search, Refresh } from '@element-plus/icons-vue'\nimport { ElMessage, ElMessageBox } from 'element-plus'\n\ndefinePageMeta({\n  title: '退款管理'\n})\n\ntype RefundStatus = 'pending' | 'rejected' | 'refunded'\n\ninterface OrderInfo {\n  orderNo: string\n  productName: string\n  productSpec: string\n  paidAmount: number\n  payTime: string\n  expiryTime: string\n  paymentMethod: string\n}\n\ninterface RefundItem {\n  id: string\n  refundNo: string\n  orderNo: string\n  uid: string\n  amount: number\n  applyTime: string\n  status: RefundStatus\n  reason: string\n  rejectReason?: string\n  orderInfo: OrderInfo\n}\n\nconst loading = ref(false)\nconst filterParams = reactive({\n  status: '',\n  uid: ''\n})\n\nconst dialogVisible = ref(false)\nconst rejectDialogVisible = ref(false)\nconst agreeDialogVisible = ref(false)\nconst rejectForm = reactive({\n  reason: ''\n})\nconst currentRow = ref<RefundItem | null>(null)\n\n// Mock Data\nconst refundList = ref<RefundItem[]>([\n  {\n    id: '1',\n    refundNo: 'REF202505200001',\n    orderNo: 'ORD202505201001',\n    uid: '10086',\n    amount: 99.00,\n    applyTime: '2025-05-20 14:30:00',\n    status: 'pending',\n    reason: '买错了，不需要了',\n    orderInfo: {\n      orderNo: 'ORD202505201001',\n      productName: '高级会员年卡',\n      productSpec: '12个月',\n      paidAmount: 99.00,\n      payTime: '2025-05-20 10:00:00',\n      expiryTime: '2026-05-20 10:00:00',\n      paymentMethod: '微信支付'\n    }\n  },\n  {\n    id: '2',\n    refundNo: 'REF202505190005',\n    orderNo: 'ORD202505190888',\n    uid: '10010',\n    amount: 299.00,\n    applyTime: '2025-05-19 16:00:00',\n    status: 'refunded',\n    reason: '商品描述不符',\n    orderInfo: {\n      orderNo: 'ORD202505190888',\n      productName: '限量版皮肤礼包',\n      productSpec: '全套',\n      paidAmount: 299.00,\n      payTime: '2025-05-19 12:00:00',\n      expiryTime: '永久',\n      paymentMethod: '支付宝'\n    }\n  },\n  {\n    id: '3',\n    refundNo: 'REF202505180022',\n    orderNo: 'ORD202505180666',\n    uid: '9527',\n    amount: 50.00,\n    applyTime: '2025-05-18 09:30:00',\n    status: 'rejected',\n    reason: '不想玩了',\n    rejectReason: '虚拟商品一经发出概不退换',\n    orderInfo: {\n      orderNo: 'ORD202505180666',\n      productName: '游戏点券',\n      productSpec: '500点',\n      paidAmount: 50.00,\n      payTime: '2025-05-18 09:00:00',\n      expiryTime: '永久',\n      paymentMethod: '余额支付'\n    }\n  }\n])\n\nconst getStatusTag = (status: RefundStatus) => {\n  const map: Record<RefundStatus, 'info' | 'success' | 'danger' | 'warning'> = {\n    pending: 'warning',\n    refunded: 'success',\n    rejected: 'danger'\n  }\n  return map[status] || 'info'\n}\n\nconst getStatusName = (status: RefundStatus) => {\n  const map: Record<RefundStatus, string> = {\n    pending: '待处理',\n    refunded: '已退款',\n    rejected: '已驳回'\n  }\n  return map[status] || status\n}\n\nconst handleSearch = () => {\n  loading.value = true\n  setTimeout(() => {\n    loading.value = false\n    ElMessage.success('查询成功 (Mock)')\n  }, 500)\n}\n\nconst handleRefresh = () => {\n  loading.value = true\n  filterParams.status = ''\n  filterParams.uid = ''\n  setTimeout(() => {\n    loading.value = false\n    ElMessage.success('已刷新')\n  }, 500)\n}\n\nconst handleDetail = (row: RefundItem) => {\n  currentRow.value = row\n  dialogVisible.value = true\n}\n\nconst resetDialog = () => {\n  // Clear any temp state if needed\n}\n\nconst handleAgree = () => {\n  if (!currentRow.value) return\n  agreeDialogVisible.value = true\n}\n\nconst confirmAgree = () => {\n    // Mock processing\n    loading.value = true\n    setTimeout(() => {\n      loading.value = false\n      if (currentRow.value) {\n        currentRow.value.status = 'refunded'\n      }\n      agreeDialogVisible.value = false\n      dialogVisible.value = false\n      ElMessage.success('操作成功，已同意退款')\n    }, 800)\n}\n\nconst handleReject = () => {\n   rejectForm.reason = ''\n   rejectDialogVisible.value = true\n}\n\nconst confirmReject = () => {\n  if (!rejectForm.reason) {\n     ElMessage.warning('驳回原因不能为空')\n     return\n  }\n   // Mock processing\n    loading.value = true\n    setTimeout(() => {\n      loading.value = false\n      if (currentRow.value) {\n        currentRow.value.status = 'rejected'\n        currentRow.value.rejectReason = rejectForm.reason\n      }\n      rejectDialogVisible.value = false\n      dialogVisible.value = false\n      ElMessage.warning('已驳回该退款申请')\n    }, 800)\n}\n\n</script>\n\n<style scoped>\n.refund-page {\n  padding: 0;\n}\n.page-header {\n  margin-bottom: 24px;\n}\n.page-header h2 {\n  margin: 0;\n  font-size: 28px;\n  color: var(--el-text-color-primary);\n  font-weight: 600;\n}\n\n.filter-card {\n  margin-bottom: 24px;\n}\n.filter-container {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n.filter-left {\n  display: flex;\n  align-items: center;\n}\n\n.section-title {\n  font-size: 16px;\n  font-weight: 600;\n  color: var(--el-text-color-primary);\n  margin-bottom: 12px;\n  border-left: 4px solid #409EFF;\n  padding-left: 8px;\n}\n</style>\n"],"version":3}