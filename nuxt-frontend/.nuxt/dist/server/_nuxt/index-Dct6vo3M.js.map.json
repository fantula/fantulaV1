l-button>\n            </template>\n          </el-input>\n          <div class=\"input-tip\">ËØ∑ËæìÂÖ•Á≤æÂáÜUIDÔºåÁ≥ªÁªüÂ∞ÜËá™Âä®Ê†°È™åÁî®Êà∑ÊòØÂê¶Â≠òÂú®„ÄÇ</div>\n        </div>\n\n        <el-divider content-position=\"center\">Â∑≤Ê∑ªÂä†Áî®Êà∑ÂàóË°®</el-divider>\n\n        <div class=\"recipient-list\">\n          <transition-group name=\"list\">\n            <div v-for=\"user in recipients\" :key=\"user.uid\" class=\"recipient-item\">\n              <el-avatar :size=\"32\" :src=\"user.avatar\">\n                {{ user.email?.charAt(0).toUpperCase() }}\n              </el-avatar>\n              <div class=\"recipient-info\">\n                <div class=\"recipient-name\">{{ user.email || 'Áî®Êà∑' }}</div>\n                <div class=\"recipient-uid\">UID: {{ user.uid }}</div>\n              </div>\n              <el-button link type=\"danger\" :icon=\"Close\" @click=\"removeUser(user.uid)\" />\n            </div>\n          </transition-group>\n          <div v-if=\"recipients.length === 0\" class=\"empty-list\">\n            <el-empty description=\"ÊöÇÊó†Êé•Êî∂ÂØπË±°\" :image-size=\"60\" />\n          </div>\n        </div>\n      </el-card>\n\n      <!-- Right: Message Content -->\n      <el-card shadow=\"never\" class=\"message-card\">\n        <template #header>\n          <div class=\"card-header\">\n            <span>Ê∂àÊÅØÂÜÖÂÆπ</span>\n          </div>\n        </template>\n\n        <el-form label-position=\"top\" :model=\"form\" :rules=\"rules\" ref=\"formRef\">\n          <el-form-item label=\"Ê∂àÊÅØÁ±ªÂûã\" prop=\"type\">\n            <el-select v-model=\"form.type\" placeholder=\"ÈÄâÊã©Ê∂àÊÅØÁ±ªÂûã\" style=\"width: 100%\">\n              <el-option label=\"üì¢ Á≥ªÁªüÈÄöÁü•\" value=\"system\" />\n              <el-option label=\"üì¶ ËÆ¢ÂçïÊ∂àÊÅØ\" value=\"order\" />\n              <el-option label=\"üéâ Ê¥ªÂä®Êé®Âπø\" value=\"activity\" />\n              <el-option label=\"üîí ÂÆâÂÖ®ÊèêÈÜí\" value=\"security\" />\n            </el-select>\n          </el-form-item>\n          <el-form-item label=\"Ê∂àÊÅØÊ†áÈ¢ò\" prop=\"title\">\n            <el-input v-model=\"form.title\" placeholder=\"ËØ∑ËæìÂÖ•Ê∂àÊÅØÊ†áÈ¢ò\" />\n          </el-form-item>\n          <el-form-item label=\"Ê∂àÊÅØÊ≠£Êñá\" prop=\"content\">\n            <el-input \n              v-model=\"form.content\" \n              type=\"textarea\" \n              :rows=\"8\" \n              placeholder=\"ËØ∑ËæìÂÖ•Ê∂àÊÅØÊ≠£ÊñáÂÜÖÂÆπ...\" \n              resize=\"none\"\n            />\n          </el-form-item>\n          <el-form-item label=\"Ë∑≥ËΩ¨ÈìæÊé•\" prop=\"link\">\n            <el-input v-model=\"form.link\" placeholder=\"ÂèØÈÄâÔºåÂ¶Ç /profile/orders ËÆ©Áî®Êà∑ÁÇπÂáªË∑≥ËΩ¨\" />\n            <div class=\"input-tip\">Áî®Êà∑ÁÇπÂáªÊ∂àÊÅØÊó∂Â∞ÜË∑≥ËΩ¨Âà∞Ê≠§Ë∑ØÂæÑ„ÄÇÁïôÁ©∫ÂàôÊó†Ë∑≥ËΩ¨„ÄÇ</div>\n          </el-form-item>\n          \n          <el-form-item>\n            <el-alert title=\"ÂèëÈÄÅÂêéÂ∞ÜÁîüÊàêÁ´ôÂÜÖ‰ø°Ôºå‰∏çÂèØÊí§Âõû„ÄÇ\" type=\"warning\" show-icon :closable=\"false\" style=\"margin-bottom: 15px;\" />\n            <el-button type=\"primary\" size=\"large\" @click=\"handleSend\" :loading=\"sending\" style=\"width: 100%;\">\n              Á°ÆËÆ§ÂèëÈÄÅÊ∂àÊÅØ\n            </el-button>\n          </el-form-item>\n        </el-form>\n      </el-card>\n\n    </div>\n\n    <!-- Recent Messages -->\n    <el-card shadow=\"never\" class=\"history-card\" style=\"margin-top: 24px;\">\n      <template #header>\n        <div class=\"card-header\">\n          <span>ÊúÄËøëÂèëÈÄÅËÆ∞ÂΩï</span>\n          <el-button text @click=\"loadMessages\">Âà∑Êñ∞</el-button>\n        </div>\n      </template>\n      \n      <el-table :data=\"messageList\" style=\"width: 100%\" v-loading=\"loadingMessages\">\n        <el-table-column prop=\"user_uid\" label=\"Áî®Êà∑UID\" width=\"120\" />\n        <el-table-column prop=\"title\" label=\"Ê†áÈ¢ò\" min-width=\"200\" />\n        <el-table-column prop=\"content\" label=\"ÂÜÖÂÆπ\" min-width=\"300\" show-overflow-tooltip />\n        <el-table-column label=\"Â∑≤ËØª\" width=\"80\" align=\"center\">\n          <template #default=\"{ row }\">\n            <el-tag :type=\"row.is_read ? 'success' : 'info'\" size=\"small\">\n              {{ row.is_read ? 'Â∑≤ËØª' : 'Êú™ËØª' }}\n            </el-tag>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"ÂèëÈÄÅÊó∂Èó¥\" width=\"180\">\n          <template #default=\"{ row }\">\n            {{ formatDate(row.created_at) }}\n          </template>\n        </el-table-column>\n      </el-table>\n    </el-card>\n  </div>\n</template>\n\n<script setup lang=\"ts\">\nimport { ref, reactive, onMounted } from 'vue'\nimport { Close } from '@element-plus/icons-vue'\nimport { ElMessage, ElMessageBox, type FormInstance, type FormRules } from 'element-plus'\nimport { adminUserApi, adminMessageApi, type AdminUser, type AdminMessage } from '@/api/admin-supabase'\n\ndefinePageMeta({\n  title: 'Ê∂àÊÅØÂèëÈÄÅÁÆ°ÁêÜ'\n})\n\n// --- Interfaces ---\ninterface RecipientInfo {\n  uid: string\n  email?: string\n  avatar?: string\n}\n\n// --- State ---\nconst uidInput = ref('')\nconst validating = ref(false)\nconst sending = ref(false)\nconst loadingMessages = ref(false)\nconst recipients = ref<RecipientInfo[]>([])\nconst messageList = ref<AdminMessage[]>([])\nconst formRef = ref<FormInstance>()\nconst form = reactive({\n  type: 'system' as 'system' | 'order' | 'activity' | 'security',\n  title: '',\n  content: '',\n  link: ''\n})\n\n// --- Rules ---\nconst rules = reactive<FormRules>({\n  title: [{ required: true, message: 'ËØ∑ËæìÂÖ•Ê∂àÊÅØÊ†áÈ¢ò', trigger: 'blur' }],\n  content: [{ required: true, message: 'ËØ∑ËæìÂÖ•Ê∂àÊÅØÊ≠£Êñá', trigger: 'blur' }]\n})\n\n// --- Helpers ---\nconst formatDate = (dateStr: string) => {\n  return new Date(dateStr).toLocaleString('zh-CN')\n}\n\n// --- Load Messages History ---\nconst loadMessages = async () => {\n  loadingMessages.value = true\n  try {\n    const result = await adminMessageApi.getMessages({ limit: 20 })\n    if (result.success) {\n      messageList.value = result.messages\n    }\n  } catch (e: any) {\n    console.error('Âä†ËΩΩÊ∂àÊÅØÂ§±Ë¥•:', e)\n  } finally {\n    loadingMessages.value = false\n  }\n}\n\n// --- Methods ---\nconst addUser = async () => {\n  const uid = uidInput.value.trim()\n  if (!uid) return\n  if (uid.length !== 8) {\n    ElMessage.warning('UID Ê†ºÂºèÈîôËØØÔºåÂ∫î‰∏∫8‰ΩçÊï∞Â≠ó')\n    return\n  }\n  if (recipients.value.some(u => u.uid === uid)) {\n    ElMessage.warning('ËØ•Áî®Êà∑Â∑≤Âú®ÂàóË°®‰∏≠')\n    uidInput.value = ''\n    return\n  }\n\n  validating.value = true\n  try {\n    const result = await adminUserApi.getUserByUid(uid)\n    if (result.success && result.user) {\n      recipients.value.unshift({\n        uid: result.user.uid,\n        email: result.user.email,\n      })\n      ElMessage.success(`Ê∑ªÂä†Áî®Êà∑ ${result.user.email} ÊàêÂäü`)\n      uidInput.value = ''\n    } else {\n      ElMessage.error('Áî®Êà∑‰∏çÂ≠òÂú®ÔºåËØ∑Ê£ÄÊü•UID')\n    }\n  } catch (e: any) {\n    ElMessage.error(e.message || 'Êü•ËØ¢Áî®Êà∑Â§±Ë¥•')\n  } finally {\n    validating.value = false\n  }\n}\n\nconst removeUser = (uid: string) => {\n  recipients.value = recipients.value.filter(u => u.uid !== uid)\n}\n\nconst handleSend = async () => {\n  if (recipients.value.length === 0) {\n    ElMessage.warning('ËØ∑Ëá≥Â∞ëÊ∑ªÂä†‰∏Ä‰∏™Êé•Êî∂Áî®Êà∑')\n    return\n  }\n  if (!formRef.value) return\n  \n  await formRef.value.validate(async (valid) => {\n    if (valid) {\n      try {\n        await ElMessageBox.confirm(\n          `Á°ÆËÆ§Âêë ${recipients.value.length} ‰ΩçÁî®Êà∑ÂèëÈÄÅÊ∂àÊÅØÂêóÔºü\\nÂèëÈÄÅÂêé‰∏çÂèØÊí§Âõû„ÄÇ`,\n          'ÂèëÈÄÅÁ°ÆËÆ§',\n          {\n            confirmButtonText: 'Á°ÆËÆ§ÂèëÈÄÅ',\n            cancelButtonText: 'ÂèñÊ∂à',\n            type: 'warning'\n          }\n        )\n        \n        sending.value = true\n        \n        // ÈÄê‰∏™ÂèëÈÄÅÊ∂àÊÅØ\n        let successCount = 0\n        let failCount = 0\n        \n        for (const user of recipients.value) {\n          const result = await adminMessageApi.sendMessage(user.uid, form.title, form.content, form.type, form.link || undefined)\n          if (result.success) {\n            successCount++\n          } else {\n            failCount++\n            console.error(`ÂèëÈÄÅÁªô ${user.uid} Â§±Ë¥•:`, result.error)\n          }\n        }\n        \n        if (successCount > 0) {\n          ElMessage.success(`ÊàêÂäüÂèëÈÄÅ ${successCount} Êù°Ê∂àÊÅØ${failCount > 0 ? `ÔºåÂ§±Ë¥• ${failCount} Êù°` : ''}`)\n        } else {\n          ElMessage.error('Ê∂àÊÅØÂèëÈÄÅÂ§±Ë¥•')\n        }\n        \n        // Reset\n        recipients.value = []\n        form.type = 'system'\n        form.title = ''\n        form.content = ''\n        form.link = ''\n        formRef.value?.resetFields()\n        \n        // Refresh history\n        await loadMessages()\n        \n      } catch (e) {\n        // User cancelled\n      } finally {\n        sending.value = false\n      }\n    }\n  })\n}\n\n// --- Init ---\nonMounted(() => {\n  loadMessages()\n})\n</script>\n\n<style scoped>\n.message-page { /* Padding handled jointly by Layout */ }\n.page-header { margin-bottom: 24px; }\n.page-header h2 { margin: 0; font-size: 28px; color: var(--el-text-color-primary); font-weight: 600; }\n\n.content-layout {\n  display: flex;\n  gap: 24px;\n  align-items: flex-start;\n}\n\n.recipient-card {\n  width: 320px;\n  flex-shrink: 0;\n  min-height: 500px;\n}\n\n.message-card {\n  flex: 1;\n  min-height: 500px;\n}\n\n.card-header {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  font-weight: 600;\n}\n\n.input-tip {\n  font-size: 12px;\n  color: #909399;\n  margin-top: 4px;\n}\n\n.recipient-list {\n  max-height: 350px;\n  overflow-y: auto;\n}\n\n.recipient-item {\n  display: flex;\n  align-items: center;\n  padding: 8px;\n  border-radius: 4px;\n  margin-bottom: 8px;\n  background-color: #f5f7fa;\n  transition: all 0.2s;\n}\n.recipient-item:hover {\n  background-color: #ecf5ff;\n}\n\n.recipient-info {\n  flex: 1;\n  margin-left: 10px;\n  overflow: hidden;\n}\n\n.recipient-name {\n  font-size: 14px;\n  font-weight: 500;\n  color: var(--el-text-color-primary);\n  white-space: nowrap;\n  overflow: hidden;\n  text-overflow: ellipsis;\n}\n.recipient-uid {\n  font-size: 12px;\n  color: #909399;\n  font-family: monospace;\n}\n\n.empty-list {\n  padding: 40px 0;\n  display: flex;\n  justify-content: center;\n}\n\n/* Transitions */\n.list-enter-active,\n.list-leave-active {\n  transition: all 0.3s ease;\n}\n.list-enter-from,\n.list-leave-to {\n  opacity: 0;\n  transform: translateX(-20px);\n}\n</style>\n"],"version":3}